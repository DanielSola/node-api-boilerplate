    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.9.0",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "5.6.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-promise": "4.2.1",
    "jest": "24.9.0"
  },
